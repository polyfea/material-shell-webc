{
  "microfrontends": {
    "material-design": {
      "module": "md/material-all.mjs",
      "resources": [
        {
          "kind": "stylesheet",
          "href": "md/fonts/material-symbols/style.css"
        },
        {
          "kind": "stylesheet",
          "href": "md/fonts/roboto/style.css"
        },
        {
          "kind": "stylesheet",
          "href": "md/theme/tokens.css"
        },
        {
          "kind": "stylesheet",
          "href": "md/theme/theme.css"
        }
      ]
    },
    "polyfea-md": {
      "dependsOn": [
        "material-design"
      ],
      "module": "dist/md-shell/md-shell.esm.js"
    },
    "my-fea": {
      "dependsOn": [
        "material-design"
      ],
      "module": "dev-srv-folder/my-fea.esm.js"
    }
  },
  "contextAreas": [
    {
      "name": "shell",
      "contextArea": {
        "elements": [
          {
            "tagName": "polyfea-md-shell",
            "attributes": {
              "application-headline": "Polyfea Shell"
            },
            "microfrontend": "polyfea-md"
          }
        ]
      }
    },
    {
      "name": "main-content",
      "path": "^(\\.?/)?$",
      "contextArea": {
        "elements": [
          {
            "tagName": "polyfea-md-apps",
            "microfrontend": "polyfea-md"
          }
        ]
      }
    },
    {
      "name": "main-content",
      "path": "^(\\.?/)?my-fea-area$",
      "contextArea": {
        "elements": [
          {
            "tagName": "my-fea-root",
            "microfrontend": "my-fea"
          }
        ]
      }
    },
    {
      "name": "applications",
      "contextArea": {
        "elements": [
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "My Fea App",
              "supporting": "The best app ever",
              "material-icon": "mood",
              "tile-img-src": "https://images-assets.nasa.gov/image/PIA26234/PIA26234~orig.jpg",
              "href": "./my-fea-area"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Access Management",
              "supporting": "Provide access to the system",
              "material-icon": "key",
              "href": "./access-management",
              "tile-img-src": "https://images-assets.nasa.gov/image/PIA25725/PIA25725~orig.jpg"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Yet Another fea",
              "supporting": "In controller each microfrontend and webcomponent is configured by itself not on central place like in static config",
              "disable-tile-img": true,
              "material-icon": "manufacturing",
              "href": "./olive",
              "tile-img-src": "https://images-assets.nasa.gov/image/PIA25440/PIA25440~orig.jpg"
            }
          }
        ]
      }
    },
    {
      "name": "drawer-content",
      "contextArea": {
        "elements": [
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "My Fea App",
              "supporting": "The best app ever",
              "material-icon": "mood",
              "href": "./my-fea-area"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Yet Another fea",
              "supporting": "In controller each microfrontend and webcomponent is configured by itself not on central place like in static config",
              "disable-tile-img": true,
              "material-icon": "manufacturing",
              "href": "./olive"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-drawer-label",
            "attributes": {
              "headline": "Configuration"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Access Management",
              "supporting": "Provide access to the system",
              "material-icon": "key",
              "href": "./access-management"
            }
          },
          {
            "microfrontend": "material-design",
            "tagName": "md-divider"
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Logout",
              "material-icon": "logout",
              "href": "./logout"
            }
          }
        ]
      }
    },
    {
      "name": "rail-content",
      "contextArea": {
        "elements": [
          {
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Home",
              "supporting": "Landing zone",
              "material-icon": "home",
              "short-headline": "Home",
              "href": "./"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "My Fea App",
              "short-headline": "My Fea",
              "supporting": "The best app ever",
              "material-icon": "mood",
              "href": "./my-fea-area"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Yet Another fea",
              "short-headline": "Another",
              "supporting": "In controller each microfrontend and webcomponent is configured by itself not on central place like in static config",
              "disable-tile-img": true,
              "material-icon": "manufacturing",
              "href": "./olive"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Logout",
              "material-icon": "logout",
              "href": "./logout"
            }
          }
        ]
      }
    },
    {
      "name": "navigation-content",
      "contextArea": {
        "elements": [
          {
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Home",
              "supporting": "Landing zone",
              "material-icon": "home",
              "short-headline": "Home",
              "href": "./"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Access Management",
              "supporting": "Provide access to the system",
              "material-icon": "key",
              "href": "./access-management"
            }
          },
          {
            "microfrontend": "polyfea-md",
            "tagName": "polyfea-md-app",
            "attributes": {
              "headline": "Logout",
              "material-icon": "logout",
              "href": "./logout"
            }
          }
        ]
      }
    }
  ]
}